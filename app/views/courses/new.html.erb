<h1>Courses#new</h1>
<p>Find me in app/views/courses/new.html.erb</p>

<div class="container">
  <%= simple_form_for @course do |f| %>
  <%= f.input :title %>
  <%= f.input :category, collection: @category %>
  <%= f.input :description, as: :text %>
  <%= f.input :price_per_hour %>
  <%= f.button :submit %>
  <% end %>
</div>